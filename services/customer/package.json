{
  "name": "customer",
  "version": "1.0.0",
  "description": "The Customer microservice.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@loopback/boot": "^0.3.4",
    "@loopback/core": "^0.2.4",
    "@loopback/repository": "^0.2.4",
    "@loopback/rest": "^0.3.4",
    "@types/cors": "^2.8.3",
    "loopback-datasource-juggler": "^3.15.5"
  },
  "devDependencies": {
    "@loopback/build": "^0.3.2",
    "@loopback/testlab": "^0.3.0"
  },
  "scripts": {
    "build": "lb-tsc && ./bin/generate-example-file-db",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run prettier:fix && npm run tslint:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha \"dist/test/unit/**/*.js\"",
    "posttest": "npm run lint",
    "start": "npm run clean && npm run build && node dist/index.js",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "loopback-next",
    "example",
    "customer",
    "microservice"
  ],
  "author": "IBM",
  "license": "MIT"
}
